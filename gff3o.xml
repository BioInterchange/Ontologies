<!DOCTYPE rdf:RDF [
    <!ENTITY dc "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY faldo "http://biohackathon.org/resource/faldo#" >
    <!ENTITY ncbi "http://purl.obolibrary.org/obo/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.sequenceontology.org/gff3#"
     xml:base="http://www.sequenceontology.org/gff3"
     xmlns:dc="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ncbi="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:faldo="http://biohackathon.org/resource/faldo#">
    <owl:Ontology rdf:about="http://www.sequenceontology.org/gff3#">
        <dc:rights>Creative Commons - By Attribution [http://creativecommons.org/licenses/by/3.0/].</dc:rights>
        <rdfs:seeAlso>https://github.com/BioInterchange/Ontologies</rdfs:seeAlso>
        <dc:contributor>Joachim Baran</dc:contributor>
        <owl:versionInfo>1.0.2</owl:versionInfo>
        <dc:contributor>Robert Hoehndorf</dc:contributor>
        <dc:description xml:lang="en">An ontology that describes the Generic Feature Format Version 3 (GFF3) by Lincoln Stein (http://www.sequenceontology.org/resources/gff3.html). The ontology was inspired by previous work of Robert Hoehndorf&#39;s RDF2OWL (http://code.google.com/p/rdf2owl).</dc:description>
        <dc:title xml:lang="en">Generic Feature Format Version 3 Ontology (GFF3O)</dc:title>
        <dc:creator xml:lang="en">Joachim Baran</dc:creator>
    </owl:Ontology>
    


