    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0010 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0010">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>strand</rdfs:label>
        <rdfs:comment xml:lang="en">Strand of the feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0016"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0012 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0012">
        <rdfs:label>attributes</rdfs:label>
        <rdfs:comment xml:lang="en">Tag name/value pair attributes of a feature that are not covered by object-/data-properties of the ontology. Tags that are represented as object-/data-properties are: ID, Name, Alias, Parent, Target, Gap, Derives_from, Dbxref, Ontology_term, Ontology_term, or Is_circular</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0003"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0014 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0014">
        <rdfs:label>parent</rdfs:label>
        <rdfs:comment xml:lang="en">Link out to the parent feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0015 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0015">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment xml:lang="en">Relationship that describes which features belong to a feature set.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0001"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0025"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0021 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0021">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>region</rdfs:label>
        <rdfs:comment xml:lang="en">FALDO &quot;Region&quot; instance replacement for a feature&#39;s start, stop, strand properties.</rdfs:comment>
        <rdfs:range rdf:resource="&faldo;Region"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0025"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0023 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0023">
        <rdfs:label>species</rdfs:label>
        <rdfs:comment xml:lang="en">NCBI Taxonomy Ontology &quot;NCBITaxon_1&quot; (or sub-classes) instance that denotes the species for a feature set.</rdfs:comment>
        <rdfs:range rdf:resource="&ncbi;NCBITaxon_1"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0001"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0025"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0025 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0025">
        <rdfs:label>set_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Set class instances.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0026 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0026">
        <rdfs:label>feature_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Feature class instances.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0034 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0034">
        <rdfs:label>dbxref</rdfs:label>
        <rdfs:comment xml:lang="en">A database cross-reference to associate a sequence alteration to its  representation in another database.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0030"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0035 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0035">
        <rdfs:label>ontology_term</rdfs:label>
        <rdfs:comment xml:lang="en">A cross-reference to an ontology term that is associated with a feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0039 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0039">
        <rdfs:label>target</rdfs:label>
        <rdfs:comment xml:lang="en">Identifies the target that the features aligns to.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0038"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0044 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0044">
        <rdfs:label>target_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Target class instances.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0045 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0045">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>strand</rdfs:label>
        <rdfs:comment xml:lang="en">Strand of a target -- if applicable.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0016"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0038"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0044"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0047 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0047">
        <rdfs:label>derives_from</rdfs:label>
        <rdfs:comment xml:lang="en">Describes a temporal relationship between two features, where the object denotes the subjects origin.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0026"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0050 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0050">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>region</rdfs:label>
        <rdfs:comment xml:lang="en">FALDO &quot;Region&quot; instance replacement for a target&#39;s start, stop, strand  properties.</rdfs:comment>
        <rdfs:range rdf:resource="&faldo;Region"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0038"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0044"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0004 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0004">
        <rdfs:label>seqid</rdfs:label>
        <rdfs:comment xml:lang="en">ID of the landmark that establishes the coordinate system for the current feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>([a-zA-Z0-9.:^*$@!+_?-|]|%[a-fA-F0-9][a-fA-F0-9])+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0005 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0005">
        <rdfs:label>source</rdfs:label>
        <rdfs:comment xml:lang="en">A free text qualifier that describes the algorithm or operating procedure that generated this feature.  For example, the name of the software that generated this feature or a database name.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0006 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0006">
        <rdfs:label>type</rdfs:label>
        <rdfs:comment xml:lang="en">Type of the feature, which is either a term from the &quot;lite&quot; version of the Sequence Ontology (SOFA), a term from the full Sequence Ontology (SO) that is a child of sequence_feature (SO:0000110), or a SOFA or SO accession number.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>([a-zA-Z0-9]_?)+|SO:[0-9]{7}</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0007 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0007">
        <rdfs:label>start</rdfs:label>
        <rdfs:comment xml:lang="en">Start coordinate of the feature on the seqid landmark.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0008 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0008">
        <rdfs:label>end</rdfs:label>
        <rdfs:comment xml:lang="en">End coordinate of the feature on the seqid landmark.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0009 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0009">
        <rdfs:label>score</rdfs:label>
        <rdfs:comment xml:lang="en">Score of the feature. For example, an E-value for sequence similarity features or a P-value for ab initio gene prediction features.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range rdf:resource="&owl;real"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0011 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0011">
        <rdfs:label>phase</rdfs:label>
        <rdfs:comment xml:lang="en">Phase for &quot;CDS&quot; features. It indicates where the feature begins with reference to the reading frame. For forward strand features, phase is counted from the start field, whilst for reverse strand features, phase is counted from the end field.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0002"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="&xsd;integer">2</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0013 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0013">
        <rdfs:label>tag</rdfs:label>
        <rdfs:comment xml:lang="en">Tag name of a feature attribute.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0003"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0029"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0022 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0022">
        <rdfs:label>version</rdfs:label>
        <rdfs:comment xml:lang="en">Version of the GFF3 specification that defines the feature set contents.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0001"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0027"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&owl;rational"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">3</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="&xsd;integer">4</xsd:maxExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0024 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0024">
        <rdfs:label>build</rdfs:label>
        <rdfs:comment xml:lang="en">Name of a genome assembly build that denotes the provenance of features in a feature set. For example, &#39;NCBI 36&#39; or &#39;FlyBase r4.1&#39;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0001"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0027"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0027 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0027">
        <rdfs:label>set_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Set class instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0028 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0028">
        <rdfs:label>feature_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Feature class instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0029 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0029">
        <rdfs:label>attribute_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with Attribute class instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0031 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0031">
        <rdfs:label>dbxref_properties</rdfs:label>
        <rdfs:comment xml:lang="en">Properties that are directly associated with DBXRef class instances.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0032 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0032">
        <rdfs:label>name</rdfs:label>
        <rdfs:comment xml:lang="en">Name of an external database. For example, &quot;dbSNP&quot; or &quot;OMIM&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0030"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0031"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minLength rdf:datatype="&xsd;integer">1</xsd:minLength>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0033 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gff3#GFF3_0033">
        <rdfs:label>xref</rdfs:label>
        <rdfs:comment xml:lang="en">External database identifier. For example, for dbSNP, this identifier could be &quot;rs3131969&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0030"/>
        <rdfs:subPropertyOf rdf:resource="http://www.sequenceontology.org/gff3#GFF3_0031"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minLength rdf:datatype="&xsd;integer">1</xsd:minLength>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gff3#GFF3_0036 -->

