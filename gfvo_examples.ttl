@prefix : <http://www.biointerchange.org/gfvo#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix faldo: <http://biohackathon.org/resource/faldo#> .
@prefix vario: <http://purl.obolibrary.org/obo/> .
@base <http://www.biointerchange.org/gfvo#> .

#################################################################
#
#    Example 1
#
#    Encoding loci and basic genomic feature annotations. The
#    example covers lines 0-3 of the example gene description
#    given in the GFF3 specification.
#           (http://www.sequenceontology.org/resources/gff3.html)
#
#    Please note that this seemingly verbose data representation
#    is slighly misleading. The reuse of non-literals as well as
#    binary RDF encodings such as RDF/HDT permits a more concise
#    representation of the same data.
#
#################################################################

:ExampleSet1 rdf:type :File ;
             :gffVersion "3" ;
             :contains :ExampleSet1Feature1 ,
                       :ExampleSet1Feature2 ;
             rdfs:comment "A simple example of a hierarchical genomic feature dependency. This example is an excerpt of another example given in the GFF3 specification (http://sequenceontology.org/resources/gff3.html)."@en .

:ExampleSet1ChromEnd rdf:type faldo:ExactPosition ;
                     faldo:position "1497228" .

:ExampleSet1ChromStart rdf:type faldo:ExactPosition ;
                       faldo:position "1" .

:ExampleSet1Feature1 rdf:type :Feature ;
                     :id "gene00001" ;
                     :name "EDEN" ;
                     :type "http://www.sequenceontology.org/browser/current_release/term/SO:0000704" ;
                     :seqid :ExampleSet1Landmark1 .

:ExampleSet1Feature1_2Start rdf:type faldo:ExactPosition ;
                            faldo:position "1000" .

:ExampleSet1Feature2 rdf:type :Feature ;
                     :id "tfbs00001" ;
                     :type "http://www.sequenceontology.org/browser/current_release/term/SO:0000235" ;
                     :parent :ExampleSet1Feature1 ;
                     :locus :ExampleSet1Region3 .

:ExampleSet1Feature2End rdf:type faldo:ExactPosition ;
                        faldo:position "1012" .

:ExampleSet1Landmark1 rdf:type :Landmark ;
                      :id "ctg123" ;
                      :locus :ExampleSet1Region1 .

:ExampleSet1Region1 rdf:type faldo:Region ;
                    faldo:begin :ExampleSet1ChromEnd ,
                                :ExampleSet1ChromStart .

:ExampleSet1Region2 rdf:type faldo:ForwardStrandPosition ;
                    faldo:begin :ExampleSet1Feature1_2Start ;
                    faldo:end :ExampleSet1Feature1End .

:ExampleSet1Region3 rdf:type faldo:Region ;
                    faldo:begin :ExampleSet1Feature1_2Start ;
                    faldo:end :ExampleSet1Feature2End .

:ExampleSet1Feature1End rdf:type faldo:ExactPosition ;
                       faldo:position "9000" .

#################################################################
#
#    Example 2
#
#    Encoding of a sequence alignment. This is part of the GFF3
#    specification, denoting the alignment between the reference
#    sequence "chr3" and the target sequence "EST23".
#           (http://www.sequenceontology.org/resources/gff3.html)
#
#################################################################

:ExampleSet2 rdf:type :Set ;
             :contains :ExampleSet2Feature1 ,
                       :ExampleSet2Feature2 ;
             rdfs:comment "An example of a sequence alignment. This example is an excerpt of another example given in the GFF3 specification (http://sequenceontology.org/resources/gff3.html)."@en .

:ExampleSet2Feature1 rdf:type :Feature ;
                     :id "Match1" ;
                     :type "http://www.sequenceontology.org/browser/current_release/term/SO:0000343" ;
                     :seqid :ExampleSet2Landmark1 .

:ExampleSet2Feature2 rdf:type :Feature ;
                     :id "EST23" ;
                     :type "http://www.sequenceontology.org/browser/current_release/term/SO:0000345" ;
                     :target :ExampleSet2Target1 .

:ExampleSet2Landmark1 rdf:type :Landmark ;
                      :id "chr3" .

:ExampleSet2Target1 rdf:type :Target ;
                    :alignment :ExampleSet2AlignmentOperation1 .

:ExampleSet2AlignmentOperation1 rdf:type :Match ;
                       :span "8" ;
                       rdf:first :ExampleSet2AlignmentOperation1 ;
                       rdf:rest :ExampleSet2AlignmentOperation2 .

:ExampleSet2AlignmentOperation2 rdf:type :TargetSequenceGap ;
                       :span "3" ;
                       rdf:first :ExampleSet2AlignmentOperation2 ;
                       rdf:rest :ExampleSet2AlignmentOperation3 .

:ExampleSet2AlignmentOperation3 rdf:type :Match ;
                       :span "6" ;
                       rdf:first :ExampleSet2AlignmentOperation3 ;
                       rdf:rest :ExampleSet2AlignmentOperation4 .

:ExampleSet2AlignmentOperation4 rdf:type :ReferenceSequenceGap ;
                       :span "1" ;
                       rdf:first :ExampleSet2AlignmentOperation4 ;
                       rdf:rest :ExampleSet2AlignmentOperation5 .

:ExampleSet2AlignmentOperation5 rdf:type :Match ;
                       :span "6" ;
                       rdf:first :ExampleSet2AlignmentOperation5 ;
                       rdf:rest rdf:nil .

#################################################################
#
#    Example 3
#
#    Encoding of a sequence variant. This is part of the GVF
#    specification, denoting two variants for two separate
#    sequenced individuals: "Variant_seq=A,T;Genotype=0:1,1:1".
#
#    The variants are represented by "Heterozygous Variant" and
#    "Homozygous Variant" instances. If this distinction cannot
#    be made, for example when information is only available for
#    one chromosome, then the generic "Variant" class can be
#    instantiated instead.
#
#    The sequenced individuals in this example are denoted
#    by the gender ambiguous class "Sequenced Individual", where
#    instances of "Sequenced Female", "Sequenced Male", and
#    "Sequenced Hermaphrodite" can be used where more details are
#    available.
#
#    The use of "Chromosome" instances -- or their derivatives
#    "Maternal Chromosome"/"Paternal Chromosome" -- is used
#    for placing phased genotypes on their respective chromosome,
#    which is also shown in this example.
#
#           (http://www.sequenceontology.org/resources/gvf.html)
#
#################################################################

:ExampleSet3 rdf:type :Set ;
             :contains :ExampleSet3Feature1 ,
                       :ExampleSet3Feature2 ;
             rdfs:comment "Encoding of a sequence variant. This example is part of the GVF specification, denoting two variants for two separate sequenced individuals (http://www.sequenceontology.org/resources/gvf.html)."@en .

:ExampleSet3SequencedIndividual1 rdf:type :SequencedIndividual .

:ExampleSet3SequencedIndividual2 rdf:type :SequencedIndividual .

:ExampleSet3Feature1 rdf:type :Feature ;
                     :id "Variant1" ;
                     :individual :ExampleSet3SequencedIndividual1 ;
                     :variant :ExampleSet3Variant1 ,
                              :ExampleSet3Variant2 .

:ExampleSet3Feature2 rdf:type :Feature ;
                     :id "Variant1" ;
                     :individual :ExampleSet3SequencedIndividual2 ;
                     :variant :ExampleSet3Variant3 ,
                              :ExampleSet3Variant4 .

:ExampleSet3Variant1 rdf:type :HeterozygousVariant ;
                     :genotype :ExampleSet3ChromosomeA ;
                     :sequence "A" .

:ExampleSet3Variant2 rdf:type :HeterozygousVariant ;
                     :genotype :ExampleSet3ChromosomeB ;
                     :sequence "T" .

:ExampleSet3Variant3 rdf:type :HomozygousVariant ;
                     :genotype :ExampleSet3ChromosomeA ;
                     :sequence "T" .

:ExampleSet3Variant4 rdf:type :HomozygousVariant ;
                     :genotype :ExampleSet3ChromosomeB ;
                     :sequence "T" .

:ExampleSet3ChromosomeA rdf:type :Chromosome .

:ExampleSet3ChromosomeB rdf:type :Chromosome .

