<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dc "http://purl.org/dc/terms/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY faldo "http://biohackathon.org/faldo#" >
    <!ENTITY ncbi "http://purl.obolibrary.org/obo/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.sequenceontology.org/gvf1#"
     xml:base="http://www.sequenceontology.org/gvf1"
     xmlns:dc="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ncbi="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:faldo="http://biohackathon.org/faldo#">
    <owl:Ontology rdf:about="http://www.sequenceontology.org/gvf1#">
        <dc:rights>Creative Commons - By Attribution [http://creativecommons.org/licenses/by/3.0/].</dc:rights>
        <owl:versionInfo>1.0.1</owl:versionInfo>
        <dc:description xml:lang="en">An ontology that describes the Genome Variation Format Version 1 (GVF) by Karen Eilbeck, Paul Flicek, Gabor Marth, Martin Reese, Lincoln Stein and Mark Yandell (http://www.sequenceontology.org/resources/gvf.html). The ontology was inspired by previous work of Robert Hoehndorf&#39;s RDF2OWL (http://code.google.com/p/rdf2owl).</dc:description>
        <dc:title xml:lang="en">Genomic Variation Format Version 1 Ontology (GVF1O)</dc:title>
        <dc:contributor>Joachim Baran</dc:contributor>
        <dc:creator xml:lang="en">Joachim Baran</dc:creator>
        <rdfs:seeAlso>https://github.com/BioInterchange/Ontologies</rdfs:seeAlso>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0010 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0010">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>strand</rdfs:label>
        <rdfs:comment xml:lang="en">Strand of the feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0016"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0012 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0012">
        <rdfs:label>attributes</rdfs:label>
        <rdfs:comment xml:lang="en">Tag name/value pair attributes of a feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0003"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0014 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0014">
        <rdfs:label>parent</rdfs:label>
        <rdfs:comment xml:lang="en">Link out to the parent feature.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0015 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0015">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment xml:lang="en">Relationship that describes which features belong to a feature set.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0001"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0021 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0021">
        <rdfs:label>region</rdfs:label>
        <rdfs:comment xml:lang="en">FALDO &quot;Region&quot; instance replacement for a feature&#39;s start, stop, strand datatype properties.</rdfs:comment>
        <rdfs:range rdf:resource="&faldo;Region"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0023 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0023">
        <rdfs:label>species</rdfs:label>
        <rdfs:comment xml:lang="en">NCBI Taxonomy Ontology &quot;NCBITaxon_1&quot; (or sub-classes) instance that denotes the species for a feature set.</rdfs:comment>
        <rdfs:range rdf:resource="&ncbi;NCBITaxon_1"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0001"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0034 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0034">
        <rdfs:label>variant</rdfs:label>
        <rdfs:comment xml:lang="en">Specific information about the variant(s) of a feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0033"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0036 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0036">
        <rdfs:label>individual</rdfs:label>
        <rdfs:comment xml:lang="en">Links to information about an individual.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0035"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0041 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0041">
        <rdfs:label>effect</rdfs:label>
        <rdfs:comment xml:lang="en">An effect of a particular feature variant.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0033"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0042 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0042">
        <rdfs:label>sequence_variant</rdfs:label>
        <rdfs:comment xml:lang="en">Effect of a sequence alteration on a sequence feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0043 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0043">
        <rdfs:label>feature_type</rdfs:label>
        <rdfs:comment xml:lang="en">A term that is describing the sequence feature that is being affected.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0044 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0044">
        <rdfs:label>feature_id</rdfs:label>
        <rdfs:comment xml:lang="en">Features that are affected by this sequence alteration effect.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0046 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0046">
        <rdfs:label>start_range</rdfs:label>
        <rdfs:comment xml:lang="en">A coordinate range for ambiguous start coordinates.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0045"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0047 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0047">
        <rdfs:label>end_range</rdfs:label>
        <rdfs:comment xml:lang="en">A coordinate range for ambiguous start coordinates.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0040"/>
        <rdfs:range rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0045"/>
    </owl:ObjectProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0051 -->

    <owl:ObjectProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0051">
        <rdfs:comment xml:lang="en">Denotes on which chromosome (for a given individual) this sequence variant appears.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0004 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0004">
        <rdfs:label>seqid</rdfs:label>
        <rdfs:comment xml:lang="en">ID of the landmark that establishes the coordinate system for the current feature.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>([a-zA-Z0-9.:^*$@!+_?-|]|%[a-fA-F0-9][a-fA-F0-9])+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0005 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0005">
        <rdfs:label>source</rdfs:label>
        <rdfs:comment xml:lang="en">A free text qualifier that describes the algorithm or operating procedure that generated this feature.  For example, the name of the software that generated this feature or a database name.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0006 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0006">
        <rdfs:label>type</rdfs:label>
        <rdfs:comment xml:lang="en">Type of the feature, which is either a term from the &quot;lite&quot; version of the Sequence Ontology (SOFA), a term from the full Sequence Ontology (SO) that is a child of sequence_feature (SO:0000110), or a SOFA or SO accession number.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>([a-zA-Z0-9]_?)+|SO:[0-9]{7}</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0007 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0007">
        <rdfs:label>start</rdfs:label>
        <rdfs:comment xml:lang="en">Start coordinate of the feature on the seqid landmark.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0008 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0008">
        <rdfs:label>end</rdfs:label>
        <rdfs:comment xml:lang="en">End coordinate of the feature on the seqid landmark.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0009 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0009">
        <rdfs:label>score</rdfs:label>
        <rdfs:comment xml:lang="en">Score of the feature. For example, an E-value for sequence similarity features or a P-value for ab initio gene prediction features.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range rdf:resource="&owl;real"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0013 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0013">
        <rdfs:label>tag</rdfs:label>
        <rdfs:comment xml:lang="en">Tag name of a feature attribute.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0003"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0022 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0022">
        <rdfs:label>version</rdfs:label>
        <rdfs:comment xml:lang="en">Version of the GVF specification that defines the feature set contents.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0001"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&owl;rational"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="&xsd;integer">2</xsd:maxExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="&xsd;integer">1</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0024 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0024">
        <rdfs:label>build</rdfs:label>
        <rdfs:comment xml:lang="en">Name of a genome assembly build that denotes the provenance of features in a feature set. For example, &#39;NCBI 36&#39; or &#39;FlyBase r4.1&#39;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0001"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0025 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0025">
        <rdfs:label>variant_seq</rdfs:label>
        <rdfs:comment xml:lang="en">All sequence variations at a locus -- including the reference sequence when appropriate (for example, when the locus is heterozygous). If the feature is on the minus strand, then the sequence is the reverse-compliment of the reference genome for these coordinates.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0033"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>[ACGTUWSMKRYBDHVN-]+|~[0-9]*|\.|!|\^</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0026 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0026">
        <rdfs:label>id</rdfs:label>
        <rdfs:comment xml:lang="en">A unique identifier for the feature within the feature set.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>.+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0027 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0027">
        <rdfs:label>alias</rdfs:label>
        <rdfs:comment xml:lang="en">Secondary name of a feature, which can be HGVS/ISCN nomenclature names, but not cross-references to databases (e.g. dbSNP, OMIM) which should use the dbxref property.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>.+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0029 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0029">
        <rdfs:label>name</rdfs:label>
        <rdfs:comment xml:lang="en">Name of an external database. For example, &quot;dbSNP&quot; or &quot;OMIM&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>.+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0030 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0030">
        <rdfs:label>xref</rdfs:label>
        <rdfs:comment xml:lang="en">External database identifier. For example, for dbSNP, this identifier could be &quot;rs3131969&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0028"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>.+</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0031 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0031">
        <rdfs:label>reference_seq</rdfs:label>
        <rdfs:comment xml:lang="en">Sequence from the reference genome.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>[ACGTUWSMKRYBDHVN-]+|~[0-9]*</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0032 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0032">
        <rdfs:label>variant_reads</rdfs:label>
        <rdfs:comment xml:lang="en">Number of reads that are supporting this variant.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0033"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0037 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0037">
        <rdfs:label>total_reads</rdfs:label>
        <rdfs:comment xml:lang="en">Total number of reads.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0035"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.sequenceontology.org/gvf1#GVF1_0038 -->

    <owl:DatatypeProperty rdf:about="http://www.sequenceontology.org/gvf1#GVF1_0038">
        <rdfs:label>zygosity</rdfs:label>
        <rdfs:comment xml:lang="en">Zygosity of a feature locus.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.sequenceontology.org/gvf1#GVF1_0002"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="&xsd;string"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:pattern>heterozygous|homozygous|hemizygous</xsd:pattern>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


